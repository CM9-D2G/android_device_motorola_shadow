# Misc Punctuation
key COMMA {
    label:                              ','
    base:                               ','
    shift, capslock:                    ','
    alt, shift+alt:                    ';'
}

key PERIOD {
    label:                              '.'
    base:                               '.'
    shift, capslock:                    '.'
