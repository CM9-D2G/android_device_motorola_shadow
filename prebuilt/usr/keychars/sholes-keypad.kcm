#
# Droid2 Global Character Map
#
# Initial revision by bikedude880

type ALPHA

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
    alt, shift+alt:                    '!'
}

key B {
    label:                              'B'
    base:                               'b'
    shift, capslock:                    'B'
    alt, shift+alt:                    '+'
}

key C {
    label:                              'C'
    base:                               'c'
    shift, capslock:                    'C'
    alt, shift+alt:                    '_'
}

key D {
    label:                              'D'
    base:                               'd'
    shift, capslock:                    'D'
    alt, shift+alt:                    '$'
}

key E {
    label:                              'E'
    base:                               'e'
    shift, capslock:                    'E'
